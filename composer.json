{
    "name": "epignosis/cache",
    "description": "Epignosis cache client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "TalentLMS Team",
            "email": "talentlms@talentlms.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Epignosis\\": "src/"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": { "Epignosis\\Tests\\": "tests/" }
    },
    "require": {
        "php" : "^7.1",
        "ext-redis" : "*",
        "ext-igbinary" : "*",
        "ext-apc" : "*",
        "ext-apcu" : "*",
        "ext-zlib" : "*",
        "psr/simple-cache": "^1.0@dev",
        "predis/predis": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
